{
	"name": "property-name-changer",
	"displayName": "PropertyNameChanger",
	"description": "Change your property name easily!",
	"version": "1.0.1",
	"publisher": "urnotzane",
	"icon": "images/icon.png",
	"license": "MIT",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development",
		"test-compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "node ./out/test/runTest.js"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Formatters",
		"Extension Packs",
		"Other"
	],
	"activationEvents": [
    "onCommand:jsonToType.toSelector",
    "onCommand:jsonToType.toType",
		"onCommand:propertyNameChanger.toBottomLine",
		"onCommand:propertyNameChanger.toHump",
		"onCommand:propertyNameChanger.jsonToBottomLine",
		"onCommand:propertyNameChanger.jsonToHump",
		"onCommand:propertyNameChanger.deleteQuote"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
      {
				"command": "jsonToType.toSelector",
				"title": "selected json → selector function"
			},
			{
				"command": "jsonToType.toType",
				"title": "selected json → TypeScript Interface",
				"category": "jsonToType.toType"
      },
			{
				"command": "propertyNameChanger.toHump",
				"title": "底杠转驼峰",
				"category": "propertyNameChanger.toHump"
			},
			{
				"command": "propertyNameChanger.toBottomLine",
				"title": "驼峰转底杠",
				"category": "propertyNameChanger.toBottomLine"
			},
			{
				"command": "propertyNameChanger.jsonToHump",
				"title": "JSON底杠转驼峰",
				"category": "propertyNameChanger.jsonToHump"
			},
			{
				"command": "propertyNameChanger.jsonToBottomLine",
				"title": "JSON驼峰转底杠",
				"category": "propertyNameChanger.jsonToBottomLine"
			},
			{
				"command": "propertyNameChanger.deleteQuote",
				"title": "删除引号",
				"category": "propertyNameChanger.deleteQuote"
			}
		],
		"keybindings": [
			{
				"command": "propertyNameChanger.toHump",
				"key": "ctrl+shift+1",
				"mac": "ctrl+shift+1",
				"when": "editorTextFocus"
			},
			{
				"command": "propertyNameChanger.toBottomLine",
				"key": "ctrl+shift+2",
				"mac": "ctrl+shift+2",
				"when": "editorTextFocus"
			},
			{
				"command": "propertyNameChanger.jsonToHump",
				"key": "ctrl+shift+3",
				"mac": "ctrl+shift+3",
				"when": "editorTextFocus"
			},
			{
				"command": "propertyNameChanger.jsonToBottomLine",
				"key": "ctrl+shift+4",
				"mac": "ctrl+shift+4",
				"when": "editorTextFocus"
			},
			{
				"command": "propertyNameChanger.deleteQuote",
				"key": "ctrl+shift+'",
				"mac": "ctrl+shift+'",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "propertyNameChanger.toHump",
					"group": "5_cutcopypaste@1"
				},
				{
					"command": "propertyNameChanger.toBottomLine",
					"group": "5_cutcopypaste@1"
				},
				{
					"command": "propertyNameChanger.jsonToHump",
					"group": "5_cutcopypaste@1"
				},
				{
					"command": "propertyNameChanger.jsonToBottomLine",
					"group": "5_cutcopypaste@1"
				},
				{
					"command": "propertyNameChanger.deleteQuote",
					"group": "5_cutcopypaste@2"
				}
			]
		}
	},
	"dependencies": {
		"prettier": "^1.19.1",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.149",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/prettier": "^1.19.0",
		"@types/vscode": "^1.40.0",
		"clean-webpack-plugin": "^3.0.0",
		"generator-code": "^1.2.9",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"module": "^1.2.5",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vsce": "^1.71.0",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"yo": "^3.1.1"
	},
	"bugs": {
		"url": "https://github.com/urnotzane/property-name-changer/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/urnotzane/property-name-changer.git"
	},
	"homepage": "https://github.com/urnotzane/property-name-changer/blob/master/README.md"
}
